opam-version: "1.2"
maintainer: "benjamin@ocamlpro.com"
build: [
  [make "all"]
  [make "doc"]
  [make "BINDIR=%{bin}%" "LIBDIR=%{lib}%" "install"]
  [make "DOCDIR=%{doc}%" "install-doc"]
]
remove: [
  [make "BINDIR=%{bin}%" "LIBDIR=%{lib}%" "uninstall"]
  [make "DOCDIR=%{doc}%" "uninstall-doc"]
]
depends: [
  "ocamlfind" {build}
  "base-unix"
  "pprint" {< "20140313"}
]
available: [ocaml-version >= "4.01.0"]
